<!DOCTYPE html>
<html>
<body>
  <div class="container">
    <header style="margin-bottom: 2rem; border-bottom: 2px solid #0066cc; padding-bottom: 1rem;">
      {{#if basics.name}}
        <h1 style="font-size: 2rem; font-weight: 700; color: #0066cc; margin-bottom: 0.5rem !important;">{{basics.name}}</h1>
      {{/if}}
      <div style="display: flex; gap: 0.75rem; flex-wrap: wrap; font-size: 0.875rem; color: #555;">
        {{#if basics.email}}<span>{{basics.email}}</span>{{/if}}
        {{#if basics.phone}}<span style="padding-left: 0.75rem; border-left: 1px solid #ccc;">{{basics.phone}}</span>{{/if}}
        {{#if basics.url}}<span style="padding-left: 0.75rem; border-left: 1px solid #ccc;"><a href="{{basics.url}}" style="color: #0066cc; text-decoration: none;">{{basics.url}}</a></span>{{/if}}
        {{#if basics.location}}<span style="padding-left: 0.75rem; border-left: 1px solid #ccc;">{{basics.location}}</span>{{/if}}
      </div>
      {{#if basics.profiles.length}}
        <div style="margin-top: 0.5rem; display: flex; gap: 0.75rem; flex-wrap: wrap; font-size: 0.875rem; color: #555;">
        {{#each basics.profiles}}
          <span style="padding-left: 0.75rem; border-left: 1px solid #ccc;"><a href="{{url}}" style="color: #0066cc; text-decoration: none;">{{network}}</a></span>
        {{/each}}
        </div>
      {{/if}}
      {{#if basics.customFields.length}}
        <div style="margin-top: 0.5rem;">
        {{#each basics.customFields}}
          <div class="field"><span class="label">{{key}}:</span><span>{{value}}</span></div>
        {{/each}}
        </div>
      {{/if}}
    </header>

    {{#if basics.summary}}
      <section class="section">
        <h2 style="font-size: 1.125rem; font-weight: 700; color: #0066cc; text-transform: uppercase; letter-spacing: 0.05em; padding-bottom: 0.25rem; border-bottom: 1px solid #e0e0e0;">Summary</h2>
        <p style="font-size: 0.875rem; margin-top: 0.5rem;">{{basics.summary}}</p>
      </section>
    {{/if}}

    {{#if skills.length}}
      <section class="section">
        <h2 style="font-size: 1.125rem; font-weight: 700; color: #0066cc; text-transform: uppercase; letter-spacing: 0.05em; padding-bottom: 0.25rem; border-bottom: 1px solid #e0e0e0;">Skills</h2>
        {{#each skills}}
          <div style="margin-bottom: 1rem; margin-top: 0.75rem;">
            <h3 style="font-size: 1rem; font-weight: 600; color: #1a1a1a; margin-bottom: 0.25rem !important;">{{name}}</h3>
            {{#if level}}<div style="font-size: 0.875rem; color: #444; font-style: italic; margin-bottom: 0.25rem;">{{level}}</div>{{/if}}
            {{#if keywords.length}}
              <div style="font-size: 0.875rem; color: #333; margin-top: 0.25rem;">
                {{#each keywords}}
                  <span style="display: inline-block; background: #f5f5f5; padding: 0.1rem 0.5rem; margin-right: 0.25rem; margin-bottom: 0.25rem; border-radius: 2px; color: #333;">{{this}}</span>
                {{/each}}
              </div>
            {{/if}}
          </div>
        {{/each}}
      </section>
    {{/if}}

    {{#if work.length}}
      <section class="section">
        <h2 style="font-size: 1.125rem; font-weight: 700; color: #0066cc; text-transform: uppercase; letter-spacing: 0.05em; padding-bottom: 0.25rem; border-bottom: 1px solid #e0e0e0;">Experience</h2>
        {{#each work}}
          <div style="margin-bottom: 1rem; margin-top: 0.75rem;">
            <div style="display: flex; justify-content: space-between; align-items: baseline; margin-bottom: 0.25rem;">
              <h3 style="font-size: 1rem; font-weight: 600; color: #1a1a1a;">{{name}}</h3>
              <span style="font-size: 0.813rem; color: #666; white-space: nowrap;">{{startDate}} - {{endDate}}</span>
            </div>
            {{#if position}}<div style="font-size: 0.875rem; color: #444; font-style: italic; margin-bottom: 0.25rem;">{{position}}</div>{{/if}}
            {{#if url}}<a href="{{url}}" style="color: #0066cc; text-decoration: none; font-size: 0.813rem;">{{url}}</a>{{/if}}
            {{#if summary}}<div style="font-size: 0.875rem; color: #333; margin-top: 0.5rem; white-space: pre-wrap;">{{summary}}</div>{{/if}}
          </div>
        {{/each}}
      </section>
    {{/if}}

    {{#if projects.length}}
      <section class="section">
        <h2 style="font-size: 1.125rem; font-weight: 700; color: #0066cc; text-transform: uppercase; letter-spacing: 0.05em; padding-bottom: 0.25rem; border-bottom: 1px solid #e0e0e0;">Projects</h2>
        {{#each projects}}
          <div style="margin-bottom: 1rem; margin-top: 0.75rem;">
            <div style="display: flex; justify-content: space-between; align-items: baseline; margin-bottom: 0.25rem;">
              <h3 style="font-size: 1rem; font-weight: 600; color: #1a1a1a;">{{name}}</h3>
              <span style="font-size: 0.813rem; color: #666; white-space: nowrap;">{{startDate}} - {{endDate}}</span>
            </div>
            {{#if url}}<a href="{{url}}" style="color: #0066cc; text-decoration: none; font-size: 0.813rem;">{{url}}</a>{{/if}}
            {{#if description}}<div style="font-size: 0.875rem; color: #333; margin-top: 0.5rem; white-space: pre-wrap;">{{description}}</div>{{/if}}
          </div>
        {{/each}}
      </section>
    {{/if}}

    {{#if education.length}}
      <section class="section">
        <h2 style="font-size: 1.125rem; font-weight: 700; color: #0066cc; text-transform: uppercase; letter-spacing: 0.05em; padding-bottom: 0.25rem; border-bottom: 1px solid #e0e0e0;">Education</h2>
        {{#each education}}
          <div style="margin-bottom: 1rem; margin-top: 0.75rem;">
            <div style="display: flex; justify-content: space-between; align-items: baseline; margin-bottom: 0.25rem;">
              <h3 style="font-size: 1rem; font-weight: 600; color: #1a1a1a;">{{institution}}</h3>
              <span style="font-size: 0.813rem; color: #666; white-space: nowrap;">{{startDate}} - {{endDate}}</span>
            </div>
            {{#if studyType}}<div style="font-size: 0.875rem; color: #444; font-style: italic; margin-bottom: 0.25rem;">{{studyType}}{{#if area}} in {{area}}{{/if}}</div>{{/if}}
            {{#if score}}<p style="font-size: 0.875rem; color: #666;">GPA: {{score}}</p>{{/if}}
            {{#if url}}<a href="{{url}}" style="color: #0066cc; text-decoration: none; font-size: 0.813rem;">{{url}}</a>{{/if}}
          </div>
        {{/each}}
      </section>
    {{/if}}

    {{#if certificates.length}}
      <section class="section">
        <h2 style="font-size: 1.125rem; font-weight: 700; color: #0066cc; text-transform: uppercase; letter-spacing: 0.05em; padding-bottom: 0.25rem; border-bottom: 1px solid #e0e0e0;">Certificates</h2>
        {{#each certificates}}
          <div style="margin-bottom: 1rem; margin-top: 0.75rem;">
            <div style="display: flex; justify-content: space-between; align-items: baseline; margin-bottom: 0.25rem;">
              <h3 style="font-size: 1rem; font-weight: 600; color: #1a1a1a;">{{name}}</h3>
              {{#if date}}<span style="font-size: 0.813rem; color: #666; white-space: nowrap;">{{date}}</span>{{/if}}
            </div>
            {{#if issuer}}<div style="font-size: 0.875rem; color: #444; font-style: italic; margin-bottom: 0.25rem;">{{issuer}}</div>{{/if}}
            {{#if url}}<a href="{{url}}" style="color: #0066cc; text-decoration: none; font-size: 0.813rem;">{{url}}</a>{{/if}}
          </div>
        {{/each}}
      </section>
    {{/if}}

    {{#if awards.length}}
      <section class="section">
        <h2 style="font-size: 1.125rem; font-weight: 700; color: #0066cc; text-transform: uppercase; letter-spacing: 0.05em; padding-bottom: 0.25rem; border-bottom: 1px solid #e0e0e0;">Awards</h2>
        {{#each awards}}
          <div style="margin-bottom: 1rem; margin-top: 0.75rem;">
            <div style="display: flex; justify-content: space-between; align-items: baseline; margin-bottom: 0.25rem;">
              <h3 style="font-size: 1rem; font-weight: 600; color: #1a1a1a;">{{title}}</h3>
              {{#if date}}<span style="font-size: 0.813rem; color: #666; white-space: nowrap;">{{date}}</span>{{/if}}
            </div>
            {{#if awarder}}<div style="font-size: 0.875rem; color: #444; font-style: italic; margin-bottom: 0.25rem;">{{awarder}}</div>{{/if}}
            {{#if summary}}<div style="font-size: 0.875rem; color: #333; margin-top: 0.5rem; white-space: pre-wrap;">{{summary}}</div>{{/if}}
          </div>
        {{/each}}
      </section>
    {{/if}}

    {{#if publications.length}}
      <section class="section">
        <h2 style="font-size: 1.125rem; font-weight: 700; color: #0066cc; text-transform: uppercase; letter-spacing: 0.05em; padding-bottom: 0.25rem; border-bottom: 1px solid #e0e0e0;">Publications</h2>
        {{#each publications}}
          <div style="margin-bottom: 1rem; margin-top: 0.75rem;">
            <div style="display: flex; justify-content: space-between; align-items: baseline; margin-bottom: 0.25rem;">
              <h3 style="font-size: 1rem; font-weight: 600; color: #1a1a1a;">{{name}}</h3>
              {{#if releaseDate}}<span style="font-size: 0.813rem; color: #666; white-space: nowrap;">{{releaseDate}}</span>{{/if}}
            </div>
            {{#if publisher}}<div style="font-size: 0.875rem; color: #444; font-style: italic; margin-bottom: 0.25rem;">{{publisher}}</div>{{/if}}
            {{#if url}}<a href="{{url}}" style="color: #0066cc; text-decoration: none; font-size: 0.813rem;">{{url}}</a>{{/if}}
            {{#if summary}}<div style="font-size: 0.875rem; color: #333; margin-top: 0.5rem; white-space: pre-wrap;">{{summary}}</div>{{/if}}
          </div>
        {{/each}}
      </section>
    {{/if}}

    {{#if volunteer.length}}
      <section class="section">
        <h2 style="font-size: 1.125rem; font-weight: 700; color: #0066cc; text-transform: uppercase; letter-spacing: 0.05em; padding-bottom: 0.25rem; border-bottom: 1px solid #e0e0e0;">Volunteer</h2>
        {{#each volunteer}}
          <div style="margin-bottom: 1rem; margin-top: 0.75rem;">
            <div style="display: flex; justify-content: space-between; align-items: baseline; margin-bottom: 0.25rem;">
              <h3 style="font-size: 1rem; font-weight: 600; color: #1a1a1a;">{{organization}}</h3>
              <span style="font-size: 0.813rem; color: #666; white-space: nowrap;">{{startDate}} - {{endDate}}</span>
            </div>
            {{#if position}}<div style="font-size: 0.875rem; color: #444; font-style: italic; margin-bottom: 0.25rem;">{{position}}</div>{{/if}}
            {{#if url}}<a href="{{url}}" style="color: #0066cc; text-decoration: none; font-size: 0.813rem;">{{url}}</a>{{/if}}
            {{#if summary}}<div style="font-size: 0.875rem; color: #333; margin-top: 0.5rem; white-space: pre-wrap;">{{summary}}</div>{{/if}}
          </div>
        {{/each}}
      </section>
    {{/if}}

    {{#if languages.length}}
      <section class="section">
        <h2 style="font-size: 1.125rem; font-weight: 700; color: #0066cc; text-transform: uppercase; letter-spacing: 0.05em; padding-bottom: 0.25rem; border-bottom: 1px solid #e0e0e0;">Languages</h2>
        {{#each languages}}
          <div style="margin-bottom: 0.5rem; margin-top: 0.5rem; display: flex; justify-content: space-between; align-items: baseline;">
            <h3 style="font-size: 1rem; font-weight: 600; color: #1a1a1a;">{{language}}</h3>
            {{#if fluency}}<span style="font-size: 0.875rem; color: #444; font-style: italic;">{{fluency}}</span>{{/if}}
          </div>
        {{/each}}
      </section>
    {{/if}}

    {{#if interests.length}}
      <section class="section">
        <h2 style="font-size: 1.125rem; font-weight: 700; color: #0066cc; text-transform: uppercase; letter-spacing: 0.05em; padding-bottom: 0.25rem; border-bottom: 1px solid #e0e0e0;">Interests</h2>
        {{#each interests}}
          <div style="margin-bottom: 1rem; margin-top: 0.75rem;">
            <h3 style="font-size: 1rem; font-weight: 600; color: #1a1a1a; margin-bottom: 0.25rem !important;">{{name}}</h3>
          </div>
        {{/each}}
      </section>
    {{/if}}

    {{#if references.length}}
      <section class="section">
        <h2 style="font-size: 1.125rem; font-weight: 700; color: #0066cc; text-transform: uppercase; letter-spacing: 0.05em; padding-bottom: 0.25rem; border-bottom: 1px solid #e0e0e0;">References</h2>
        {{#each references}}
          <div style="margin-bottom: 1rem; margin-top: 0.75rem;">
            <h3 style="font-size: 1rem; font-weight: 600; color: #1a1a1a; margin-bottom: 0.25rem !important;">{{name}}</h3>
            {{#if reference}}<div style="font-size: 0.875rem; color: #333; font-style: italic; margin-top: 0.5rem;">"{{reference}}"</div>{{/if}}
          </div>
        {{/each}}
      </section>
    {{/if}}
  </div>
</body>
</html>